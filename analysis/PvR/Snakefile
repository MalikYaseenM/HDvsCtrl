rule Protocol_Appender:
    input:
        pythonscript='protocol.py'
    output:
        outfile='sample_info.csv'
    shell:
        "python {input.pythonscript}"

        
rule PolyRibocsv:
    input:
        workflow='workflow.py'
    output:
        ribodepleted='ribodepseq.csv',
        polyadenylated='polyseq.csv'
    shell:
        "python {input.workflow}"
